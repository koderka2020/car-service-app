{
  "name": "car-service",
  "version": "1.0.0",
  "description": "simple nodejs/express app",
  "main": "server/server.js",
  "scripts": {
    "test": "jest --verbose",
    "front": "webpack serve --mode development --open --hot",
    "start": "cross-env NODE_ENV=development nodemon ./server/server.js",
    "build": "cross-env NODE_ENV=production webpack --hot",
    "dev": "cross-env NODE_ENV=development concurrently \"nodemon ./server/server.js\"  \"webpack serve --hot --open\""
